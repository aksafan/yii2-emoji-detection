{
    "name": "aksafan/yii2-emoji-detection",
    "description": "Yii2 wrapper for aaronpk's Emoji Detection",
    "keywords": ["yii2", "emoji", "detection"],
    "homepage": "https://github.com/aksafan/yii2-emoji-detection",
    "type": "yii2-extension",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/aksafan/yii2-emoji-detection/issues?q=is%3Aopen",
        "forum": "http://www.yiiframework.com/forum/",
        "wiki": "http://www.yiiframework.com/wiki/",
        "irc": "irc://irc.freenode.net/yii",
        "source": "https://github.com/yiisoft/yii2"
    },
    "authors": [
        {
            "name": "Anton Khainak",
            "email": "khainak.anton@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^8.0",
        "yiisoft/yii2": "~2.0.6",
        "p3k/emoji-detector": "^1",
        "ext-json": "*",
        "ext-mbstring": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "aksafan\\emoji\\": ""
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "config": {
        "allow-plugins": {
            "yiisoft/yii2-composer": true
        }
    }
}
